        "number = 0\n",
        "for number in range(1042000,702648265):\n",
        "    temp = number\n",
        "    sum = 0\n",
        "    while temp > 0:\n",
        "        r = temp % 10\n",
        "        sum += r**len(str(number))\n",
        "        temp = temp // 10\n",
        "    if sum == number:\n",
        "        print(\"The first armstrong number is\",number)\n",
        "        break\n",
        "    "
